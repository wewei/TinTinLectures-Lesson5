type Time = int;
type Message = 
 record {
   author: principal;
   content: text;
   time: Time;
 };
service : {
  claim_owner: () -> ();
  follow: (principal) -> ();
  follows: () -> (vec principal) query;
  get_name: () -> (text) query;
  post: (text) -> ();
  posts: (Time) -> (vec Message) query;
  set_name: (text) -> ();
  timeline: (Time) -> (vec Message);
}
